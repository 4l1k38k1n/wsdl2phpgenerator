{
    "name": "wsdl2phpgenerator/wsdl2phpgenerator",
    "description": "Simple utility and class library for generating php classes from a wsdl file.",
    "keywords": ["soap", "wsdl"],
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "Wsdl2PhpGenerator\\": ["src/", "lib/"]
        }
    },
    "bin": ["wsdl2php"],
    "require": {
        "php": ">=5.3.0",
        "ext-soap": "*",
        "psr/log": "1.0.*",
        "symfony/console": "2.3.*"
    },
    "require-dev": {
        "kherge/box": "2.4.*",
        "phpunit/phpunit": "3.7.*"
    }
}
